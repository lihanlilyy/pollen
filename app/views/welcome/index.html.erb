<% provide(:title, "Home") %>
<!DOCTYPE html>
<html>
    <head>
        <title><%= full_title(yield(:title)) %></title>
    </head>
    <br>
    <main class="wrapper">
        <div class = "homecontainer parallex">
            <%= image_tag("homepic.png", :class => "home_image") %>
            <h1 class = "left">
                Welcome back, 
                <%= current_user.lastname %>
            </h1>
        </div>
    </main>
    <br>

    <aside class="col-md-4">
        <div class="row">
            <section class="user_info">
                <%= render 'shared/user_info' %>
            </section>
        </div>
    </aside>
        
</html>